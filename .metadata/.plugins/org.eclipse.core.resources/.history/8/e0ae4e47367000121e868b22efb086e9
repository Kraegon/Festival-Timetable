import javax.swing.*;


public class InputFrame extends JFrame{
	JPanel artistPanel;
	JPanel stagePanel;
	JPanel performancePanel;
	JPanel errorPanel;
	
	public InputFrame(String source){
		setDefaultCloseOperation(DISPOSE_ON_CLOSE);
		switch(source){
		case "artist":
			setContentPane(artistPanel);
			break;
		case "stage":
			setContentPane(stagePanel);
			break;
		case "performance":
			setContentPane(performancePanel);
			break;
		default:
			setContentPane(errorPanel);
		}
		artistPanel.add(new JLabel("Is artist!"));
		stagePanel.add(new JLabel("Is stage!"));
		performancePanel.add(new JLabel("Is performance!"));
		errorPanel.add(new JLabel("Uh oh."));
		
		
		setSize(400,600);
		setVisible(true);
	}
	

}
